<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AdaSigortaMaui.FlyoutHeader.FlyoutHeaderUser">

    <StackLayout Orientation="Vertical" Margin="10">
        <Label x:Name="lblText" FontAttributes="Bold"  FontSize="Large" TextColor="Green"  HorizontalOptions="Center" VerticalOptions="Center" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"></Label >
        <Label x:Name="lblEmail" TextColor="Green" FontSize="16" FontAttributes="Italic"  HorizontalOptions="Center" VerticalOptions="Center" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"></Label>
    </StackLayout>
    
</ContentView>
